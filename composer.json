{
    "name": "prinsfrank/arithmetic-operations-floating-point",
    "description": "Floating point math implementation for the prinsfrank/arithmetic-operations interface that provides basic arithmetic operations (addition, subtraction, power, dividing, etc...)",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "PrinsFrank\\ArithmeticOperationsFloatingPoint\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PrinsFrank\\ArithmeticOperationsFloatingPoint\\Tests\\": "tests/"
        }
    },
    "require": {
        "prinsfrank/arithmetic-operations": "^0.1.0"
    },
    "provide": {
        "prinsfrank/arithmetic-operations-implementation": "self.version"
    },
    "scripts": {
        "cs": "php-cs-fixer fix --allow-risky=yes --dry-run",
        "cs:fix": "php-cs-fixer fix --allow-risky=yes",
        "unit": "phpunit --testsuite=Unit"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.12",
        "phpunit/phpunit": "^9.5"
    }
}
